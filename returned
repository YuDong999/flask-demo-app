]0;root@test-k8s:/git/flask-demo-app[root@test-k8s flask-demo-app]# kubectl replace --force -f jenkins-fixed-sts.yaml -n jenkins [A[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[25Pcat > jenkins-fixed-sts.yaml << EOF
apiVersion: apps/v1
kind: StatefulSet
metadata:
  annotations:
    meta.helm.sh/release-name: jenkins
    meta.helm.sh/release-namespace: jenkins
  labels:
    app.kubernetes.io/component: jenkins-controller
    app.kubernetes.io/instance: jenkins
    app.kubernetes.io/managed-by: Helm
    app.kubernetes.io/name: jenkins
    helm.sh/chart: jenkins-5.8.104
  name: jenkins
  namespace: jenkins
spec:
  persistentVolumeClaimRetentionPolicy:
    whenDeleted: Retain
    whenScaled: Retain
  podManagementPolicy: OrderedReady
  replicas: 1
  revisionHistoryLimit: 10
  selector:
    matchLabels:
      app.kubernetes.io/component: jenkins-controller
      app.kubernetes.io/instance: jenkins
  serviceName: jenkins
  template:
    metadata:      annotations:        checksum/config: c3118755c1ab881651eb9bed6db3f8a226469d76b851f6ba551256619a10a52f      labels:        app.kubernetes.io/component: jenkins-controller        app.kubernetes.io/instance: jenkins        app.kubernetes.io/managed-by: Helm        app.kubernetes.io/name: jenkins        helm.sh/chart: jenkins-5.8.104    spec:      automountServiceAccountToken: true      volumes:      - name: docker-sock        hostPath:          path: /var/run/docker.sock          type: File      - name: kubeconfig        hostPath:          path: /root/.kube          type: Directory      - name: jenkins-secrets        projected:          defaultMode: 420          sources:          - secret:              items:              - key: jenkins-admin-user                path: chart-admin-username              - key: jenkins-admin-password                path: chart-admin-password              name: jenkins      - name: jenkins-cache        emptyDir: {}      - name: sc-config-volume        emptyDir: {}      - name: tmp-volume        emptyDir: {}      containers:      - args:        - --httpPort=8080        env:        - name: SECRETS          value: /run/secrets/additional        - name: POD_NAME          valueFrom:            fieldRef:              apiVersion: v1              fieldPath: metadata.name        - name: JAVA_OPTS          value: '-Dcasc.reload.token=$(POD_NAME) '        - name: JENKINS_OPTS          value: '--webroot=/var/jenkins_cache/war '        - name: JENKINS_SLAVE_AGENT_PORT          value: "50000"        - name: CASC_JENKINS_CONFIG          value: /var/jenkins_home/casc_configs        - name: KUBECONFIG          value: /home/jenkins/.kube/config        image: 192.168.94.199:30003/test/jenkins-custom:2        imagePullPolicy: IfNotPresent        livenessProbe:          failureThreshold: 5          httpGet:            path: /login            port: http            scheme: HTTP          periodSeconds: 10          successThreshold: 1          timeoutSeconds: 5        name: jenkins        ports:        - containerPort: 8080          name: http          protocol: TCP        - containerPort: 50000          name: agent-listener          protocol: TCP        readinessProbe:          failureThreshold: 3          httpGet:            path: /login            port: http            scheme: HTTP          periodSeconds: 10          successThreshold: 1          timeoutSeconds: 5        resources:          limits:            cpu: "1"            memory: 1Gi          requests:            cpu: 500m            memory: 512Mi        securityContext:          allowPrivilegeEscalation: true          privileged: true          readOnlyRootFilesystem: false          runAsGroup: 0          runAsUser: 0        startupProbe:          failureThreshold: 12          httpGet:            path: /login            port: http            scheme: HTTP          periodSeconds: 10          successThreshold: 1          timeoutSeconds: 5        terminationMessagePath: /dev/termination-log        terminationMessagePolicy: File        volumeMounts:        - name: jenkins-home          mountPath: /var/jenkins_home        - name: jenkins-cache          mountPath: /var/jenkins_cache        - name: sc-config-volume          mountPath: /var/run/secrets/chart-admin-settings        - name: tmp-volume          mountPath: /tmp        - name: docker-sock          mountPath: /var/run/docker.sock        - name: kubeconfig          mountPath: /home/jenkins/.kube        - name: jenkins-secrets          mountPath: /run/secrets/additional          readOnly: true  updateStrategy:    rollingUpdate:      partition: 0    type: RollingUpdate  volumeClaimTemplates:  - metadata:      name: jenkins-home    spec:      accessModes:      - ReadWriteOnce      resources:        requests:          storage: 8Gi      storageClassName: nfs-clientEOF[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[C[Ckubectl replace --force -f jenkins-fixed-sts.yaml -n jenkins [K

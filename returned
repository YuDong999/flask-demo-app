  240        schema1:
  241          enabled: true
  242    ctl-config.yml: |
  243      ---
  244      protocol: "http"
  245      port: 8080
  246      log_level: info
  247      registry_config: "/etc/registry/config.yml"
  248  kind: ConfigMap
  249  metadata:
  250    annotations:
  251      meta.helm.sh/release-name: harbor
  252      meta.helm.sh/release-namespace: harbor
  253    labels:
  254      app: harbor
  255      app.kubernetes.io/instance: harbor

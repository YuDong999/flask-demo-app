  330  kubectl get configmap mysql-config -n mysql-cluster -o yaml | grep -E "master.cnf|slave.cnf"
  331  kubectl get configmap mysql-config -n mysql-cluster -o jsonpath='{.data.master.cnf}'
  332  kubectl delete configmap mysql-config -n mysql-cluster
  333  kubectl apply -f - <<EOF
  334  apiVersion: v1
  335  kind: ConfigMap
  336  metadata:
  337    name: mysql-config
  338    namespace: mysql-cluster
  339  data:
  340    master.cnf: |
  341      [mysqld]
  342      server-id=1
  343      log_bin=mysql-bin
  344      binlog_do_db=test
  345      skip-name-resolve
  346      character-set-server=utf8mb4
  347      collation-server=utf8mb4_unicode_ci
  348      default_authentication_plugin=mysql_native_password
  349    slave.cnf: |
  350      [mysqld]
  351      server-id=2
  352      log_bin=mysql-bin
  353      relay_log=mysql-relay-bin
  354      read_only=1
  355      skip-name-resolve
  356      character-set-server=utf8mb4
  357      collation-server=utf8mb4_unicode_ci
  358      default_authentication_plugin=mysql_native_password
  359    init-script.sh: |
  360      set -e  # 出错即退出，避免继续执行
  361      wait_for_mysql() {
  362        local max_retries=30
  363        local retry=0
  364        while ! mysqladmin ping -uroot -p"\$MYSQL_ROOT_PASSWORD" --silent; do
  365          if [ \$retry -ge \$max_retries ]; then
  366            echo "ERROR: MySQL 启动超时！"
  367            exit 1
  368          fi
  369          echo "等待 MySQL 启动（\$((retry+1))/\$max_retries）..."
  370          sleep 2
  371          retry=\$((retry+1))
  372        done
  373      }
  374      init_master() {
  375        echo "开始初始化主库（mysql-0）..."
  376        wait_for_mysql
  377        mysql -uroot -p"\$MYSQL_ROOT_PASSWORD" -e "
  378          CREATE USER IF NOT EXISTS 'repl'@'%' IDENTIFIED BY '\$MYSQL_REPL_PASSWORD';
  379          GRANT REPLICATION SLAVE ON *.* TO 'repl'@'%';
  380          FLUSH PRIVILEGES;
  381          SELECT user, host FROM mysql.user WHERE user = 'repl';
  382          SHOW MASTER STATUS;
  383        "
  384        echo "主库初始化完成！"
  385      }
  386      init_slave() {
  387        echo "开始初始化从库（非 mysql-0）..."
  388        wait_for_mysql
  389        MASTER_HOST="mysql-0.mysql-service.mysql-cluster.svc.cluster.local"
  390        local max_retries=30
  391        local retry=0
  392        while ! mysqladmin ping -h "\$MASTER_HOST" -uroot -p"\$MYSQL_ROOT_PASSWORD" --silent; do
  393          if [ \$retry -ge \$max_retries ]; then
  394            echo "ERROR: 主库 \$MASTER_HOST 未就绪！"
  395            exit 1
  396          fi
  397          echo "等待主库 \$MASTER_HOST 就绪（\$((retry+1))/\$max_retries）..."
  398          sleep 2
  399          retry=\$((retry+1))
  400        done
  401        MASTER_STATUS=\$(mysql -h "\$MASTER_HOST" -uroot -p"\$MYSQL_ROOT_PASSWORD" -Nse "SHOW MASTER STATUS")
  402        MASTER_LOG_FILE=\$(echo "\$MASTER_STATUS" | awk '{print \$1}')
  403        MASTER_LOG_POS=\$(echo "\$MASTER_STATUS" | awk '{print \$2}')
  404        mysql -uroot -p"\$MYSQL_ROOT_PASSWORD" -e "
  405          STOP SLAVE IF EXISTS;
  406          CHANGE MASTER TO
  407            MASTER_HOST='\$MASTER_HOST',
  408            MASTER_USER='repl',
  409            MASTER_PASSWORD='\$MYSQL_REPL_PASSWORD',
  410            MASTER_LOG_FILE='\$MASTER_LOG_FILE',
  411            MASTER_LOG_POS=\$MASTER_LOG_POS,
  412            MASTER_CONNECT_RETRY=10;
  413          START SLAVE;
  414          SHOW SLAVE STATUS\G
  415        "
  416        echo "从库初始化完成！"
  417      }
  418      if [ "\$(hostname | awk -F'-' '{print \$NF}')" = "0" ]; then
  419        init_master
  420      else
  421        init_slave
  422      fi
  423  EOF
  424  kubectl get configmap mysql-config -n mysql-cluster -o jsonpath='{.data.master.cnf}'
  425  kubectl get configmap mysql-config -n mysql-cluster -o jsonpath='{.data.slave.cnf}'
  426  kubectl get configmap mysql-config -n mysql-cluster -o jsonpath='{.data.init-script.sh}'

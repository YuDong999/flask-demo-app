 1002  sed -i '/      volumes:/a \
        - name: docker-sock\n          hostPath:\n            path: /var/run/docker.sock\n            type: File\n        - name: kubeconfig\n          hostPath:\n            path: /root/.kube\n            type: Directory' jenkins-sts.yaml
 1003  sed -i '/      - name: jenkins/a \
        volumeMounts:\n          - name: docker-sock\n            mountPath: /var/run/docker.sock\n          - name: kubeconfig\n            mountPath: /home/jenkins/.kube' jenkins-sts.yaml

> metadata:
>   name: jenkins-secrets-binding
>   namespace: jenkins
> subjects:
> - kind: ServiceAccount
>   name: jenkins
>   namespace: jenkins
> roleRef:
>   kind: Role
>   name: jenkins-secrets-access
>   apiGroup: rbac.authorization.k8s.io
> EOF

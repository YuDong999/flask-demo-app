  257      app.kubernetes.io/name: harbor
  258      app.kubernetes.io/part-of: harbor
  259      app.kubernetes.io/version: 2.14.0
  260      chart: harbor
  261      heritage: Helm
  262      release: harbor
  263    name: harbor-registry
  264    namespace: harbor
  265  EOF
  266  kubectl apply -f /tmp/harbor-registry-normal.yaml
